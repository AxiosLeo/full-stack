{
  "name": "ts-study-project",
  "version": "0.0.1",
  "description": "Project for study Typescript",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "lint": "eslint --fix src/*",
    "build": "tsc -p tsconfig.json",
    "dev": "./node_modules/nodemon/bin/nodemon.js",
    "test": "mocha -r ts-node/register tests/**/*.test.ts --parallel",
    "test-cov": "nyc mocha -r ts-node/register tests/**/*.test.ts --parallel"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AxiosCros/node-tools.git"
  },
  "dependencies": {
    "@axiosleo/cli-tool": "^1.3.2",
    "cluster": "^0.7.7",
    "crypto": "^1.0.1",
    "koa": "^2.13.1",
    "koa-router": "^10.0.0",
    "parse-url-parts": "^2.0.3",
    "sequelize": "^6.6.2",
    "typescript": "^4.3.5",
    "uuid": "^8.3.2"
  },
  "author": "axiosleo",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/AxiosCros/node-tools/issues"
  },
  "homepage": "https://github.com/AxiosCros/node-tools#readme",
  "devDependencies": {
    "@types/chai": "^4.2.21",
    "@types/i18n": "^0.13.0",
    "@types/koa": "^2.13.3",
    "@types/mocha": "^8.2.3",
    "@types/node": "^15.6.1",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.26.0",
    "@typescript-eslint/parser": "^4.26.0",
    "chai": "^4.3.4",
    "mocha": "^9.0.2",
    "nodemon": "^2.0.7",
    "nyc": "^15.1.0",
    "ts-node": "^10.1.0"
  },
  "nyc": {
    "all": false,
    "check-coverage": true,
    "include": [
      "src/**/*.ts"
    ],
    "exclude": [
      "tests/**/*.test.ts"
    ],
    "report-dir": "report/coverage",
    "branches": 81,
    "lines": 95,
    "functions": 90,
    "statements": 95
  }
}
